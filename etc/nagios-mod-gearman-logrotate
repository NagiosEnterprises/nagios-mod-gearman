/var/log/nagios-mod-gearman/nagios-mod-gearman-worker.log {
    missingok
    notifempty
    sharedscripts
    postrotate
        systemctl condreload nagios-mod-gearman-worker > /dev/null 2>/dev/null || true
    endscript
    compress
    daily
}

